{
    "name": "epmnzava/ae_ds",
    "description": "Ali express drop shipping library for php",
    "keywords": [
        "epmnzava",
        "ae_ds"
    ],
    "homepage": "https://github.com/epmnzava/ae_ds",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Emmanuel paul Mnzava",
            "email": "epmnzava@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Epmnzava\\AeDs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Epmnzava\\AeDs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
